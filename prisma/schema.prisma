generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "linux-musl"]
  previewFeatures = ["fullTextIndex"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id             String   @id
  nome           String
  email          String   @unique
  senha          String
  data_inclusao  DateTime @default(now())
  data_alteracao DateTime @updatedAt
}
